
DECLARE
       W_INSTITUCION VARCHAR2(50);
BEGIN 
     FOR C IN (SELECT DISTINCT INSTITUCION   FROM JAIIO) LOOP
     W_INSTITUCION := C.INSTITUCION;
     
     INSERT INTO  INSTITUCION (ID_INSTITUCION,COD,DESCRIP) VALUES ( -1,W_INSTITUCION,'Universadad Nacional de Formosa');
     
     END LOOP;
END;
