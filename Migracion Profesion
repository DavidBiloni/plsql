DECLARE
      W_PROFESION VARCHAR2(50);
      
BEGIN 
      
     FOR C IN (SELECT * FROM JAIIO) LOOP
     
      W_PROFESION := C.PROFESION;
     
     INSERT INTO PROFESION(ID_PROFESION,COD,DESCRIPCION) VALUES (-1,DECODE(W_PROFESION,'Alumno','A','Profesor','P'),W_PROFESION);
     
    END LOOP;
    
END;
